{
  "name": "@signet/connector-openclaw",
  "version": "0.1.67",
  "description": "Signet connector for OpenClaw - configures workspace and memory hooks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "bun --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@signet/connector-base": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "signet",
    "openclaw",
    "clawdbot",
    "connector",
    "ai-memory",
    "agent-identity"
  ],
  "license": "Apache-2.0"
}
